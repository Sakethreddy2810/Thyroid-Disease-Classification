# Check for missing values
print(df.isnull().sum())
# Fill missing numerical values with the column mean
df.fillna(method='ffill', inplace=True) # Forward fill for stock data continuity
df.fillna(0, inplace=True) # Replace remaining missing dividends/splits with 0
# Confirm no missing values remain
print(df.isnull().sum())
